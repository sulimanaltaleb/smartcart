                "transition": "0.4s",
                "box-shadow": "none",
                "outline": "none",
                "border-radius": "25px",
                "height": "50px",
                "&::-webkit-input-placeholder, &::-webkit-input-placeholder": {
                  "color": "@formPlaceholder"
                },
